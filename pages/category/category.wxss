.container{
  height: 100%;
  position: relative;
  overflow: hidden;
}
.input{
  width: 90%;
  height: 25px;
  margin: 8px auto;
  border-radius: 12px;
  background: #f4f4f4;
  line-height: 25px;
  text-align: center;
}
.searchT{
  font-size: 13px;
  color: #e0e0e0;
  margin: 0 5px; 
}
.search{
  width: 750rpx;
  border-top:1px solid #eee; 
  border-bottom:1px solid #eee;
  position: fixed;
  top: 0;
}
page{
  height: 100%;
}
#nav{
  height: 100%;
  width: 22%;
  display: inline-block;
  padding-top: 43px;
  box-sizing: border-box;
}
#msg{
  height: 100%;
  width: 78%;
  display: inline-block;
  padding-top: 43px;
  box-sizing: border-box;
}
.nav{
  height: 100%;
  width: 100%;
  overflow-y: auto;
}
.msg{
  height: 100%;
  width: 100%;
  overflow-y: auto;
}
.navItem{
  width: 100%;
  height: 100rpx;
  text-align: center;
  background: #f4f4f4;
  font-size: 25rpx;
  color: #000;
}
.navItem1{
  width: 100%;
  height: 100rpx;
  text-align: center;
  background: #fff;
  font-size: 30rpx;
  color: red;
}
.navT{
  line-height: 100rpx;
}
::-webkit-scrollbar{
  display:none;
}
.title{
   font-weight: 600;
   font-size: 29rpx;
   line-height: 90rpx;
   padding-left: 36rpx;
   display: block;
   margin-bottom: 25rpx;
}
.product{
  width: 140rpx;
  height: 190rpx;
  display: inline-block;
  text-align: center;
  margin-left: 40rpx;
  margin-bottom: 30rpx;
}
.img{
  width: 140rpx;
  height: 140rpx;
  display: block;
}
.msgT{
  width: 140rpx;
  height: 50rpx;
  font-size: 25rpx;
  line-height: 50rpx;
}
#productList{
  padding: 0 5rpx;
}
